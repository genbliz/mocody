{
  "name": "mocody",
  "version": "0.6.0",
  "description": "Implementation of single table design, and unified query access for MongoDB, CouchDB, and dynamoDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx rimraf ./dist && npx tsc -b",
    "prepare": "npm run build",
    "version": "npm run build",
    "pub": "npm publish",
    "build-dev": "npx rimraf ./dist && npx tsc -b",
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "eslint --fix 'src/**/*.ts'",
    "prettier-format-fix": "prettier --write \"src/**/*.{ts,js,json}\"",
    "prettier-different": "prettier --list-different \"src/**/*.{ts,js,json}\"",
    "prettier-unformatted": "prettier --check \"src/**/*.{ts,js,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genbliz/mocody.git"
  },
  "keywords": [
    "dynamodb",
    "mongodb",
    "couchdb",
    "orm",
    "single table design"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/genbliz/mocody/issues"
  },
  "homepage": "https://github.com/genbliz/mocody",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.767.0",
    "@aws-sdk/lib-dynamodb": "^3.767.0",
    "@aws-sdk/util-dynamodb": "^3.767.0",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "mongodb": "^6.14.2",
    "nano": "^10.1.4",
    "nanoid": "^5.1.4",
    "throat": "^6.0.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
