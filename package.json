{
  "name": "mocody",
  "version": "0.4.0",
  "description": "Implementation of single table design, and unified query access for MongoDB, CouchDB, and dynamoDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf ./dist && npx tsc -b",
    "prepare": "npm run build",
    "version": "npm run build",
    "pub": "npm publish",
    "pub01": "npx np",
    "build-dev": "rimraf ./dist && npx tsc -b",
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "eslint --fix 'src/**/*.ts'",
    "prettier-format-fix": "prettier --write \"src/**/*.{ts,js,json}\"",
    "prettier-different": "prettier --list-different \"src/**/*.{ts,js,json}\"",
    "prettier-unformatted": "prettier --check \"src/**/*.{ts,js,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genbliz/mocody.git"
  },
  "keywords": [
    "dynamodb",
    "mongodb",
    "couchdb",
    "orm",
    "single table design"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/genbliz/mocody/issues"
  },
  "homepage": "https://github.com/genbliz/mocody",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.172.0",
    "@aws-sdk/util-dynamodb": "^3.172.0",
    "joi": "^17.6.1",
    "lodash": "^4.17.21",
    "mongodb": "4.10.0",
    "nano": "^10.0.0",
    "nanoid": "^3.3.4",
    "throat": "^6.0.1",
    "tslib": "^2.4.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.185",
    "@types/node": "^16.11.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "@faker-js/faker": "^7.5.0",
    "np": "^7.6.2",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}
