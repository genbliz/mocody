{
  "name": "mocody",
  "version": "0.4.18",
  "description": "Implementation of single table design, and unified query access for MongoDB, CouchDB, and dynamoDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf ./dist && npx tsc -b",
    "prepare": "npm run build",
    "version": "npm run build",
    "pub": "npm publish",
    "build-dev": "rimraf ./dist && npx tsc -b",
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "eslint --fix 'src/**/*.ts'",
    "prettier-format-fix": "prettier --write \"src/**/*.{ts,js,json}\"",
    "prettier-different": "prettier --list-different \"src/**/*.{ts,js,json}\"",
    "prettier-unformatted": "prettier --check \"src/**/*.{ts,js,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genbliz/mocody.git"
  },
  "keywords": [
    "dynamodb",
    "mongodb",
    "couchdb",
    "orm",
    "single table design"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/genbliz/mocody/issues"
  },
  "homepage": "https://github.com/genbliz/mocody",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.272.0",
    "@aws-sdk/util-dynamodb": "^3.272.0",
    "joi": "^17.7.1",
    "lodash": "^4.17.21",
    "mongodb": "4.12.0",
    "nano": "^10.1.2",
    "nanoid": "^3.3.4",
    "pouchdb": "^8.0.1",
    "pouchdb-find": "^8.0.1",
    "throat": "^6.0.2",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.13.0",
    "@types/pouchdb": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^2.8.4",
    "pouchdb-adapter-node-websql": "^7.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
