{
  "name": "mocody",
  "version": "0.5.5",
  "description": "Implementation of single table design, and unified query access for MongoDB, CouchDB, and dynamoDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf ./dist && npx tsc -b",
    "prepare": "npm run build",
    "version": "npm run build",
    "pub": "npm publish",
    "build-dev": "rimraf ./dist && npx tsc -b",
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "eslint --fix 'src/**/*.ts'",
    "prettier-format-fix": "prettier --write \"src/**/*.{ts,js,json}\"",
    "prettier-different": "prettier --list-different \"src/**/*.{ts,js,json}\"",
    "prettier-unformatted": "prettier --check \"src/**/*.{ts,js,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genbliz/mocody.git"
  },
  "keywords": [
    "dynamodb",
    "mongodb",
    "couchdb",
    "orm",
    "single table design"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/genbliz/mocody/issues"
  },
  "homepage": "https://github.com/genbliz/mocody",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.421.0",
    "@aws-sdk/lib-dynamodb": "^3.421.0",
    "@aws-sdk/util-dynamodb": "^3.421.0",
    "joi": "^17.10.2",
    "lodash": "^4.17.21",
    "mongodb": "^6.1.0",
    "nano": "^10.1.2",
    "nanoid": "^3.3.4",
    "throat": "^6.0.2",
    "tslib": "^2.6.2"
  },
  "optionalDependencies": {
    "pouchdb": "^8.0.1",
    "pouchdb-adapter-node-websql": "^7.0.0",
    "pouchdb-find": "^8.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/lodash": "^4.14.199",
    "@types/node": "^18.13.0",
    "@types/pouchdb": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "pouchdb": "^8.0.1",
    "pouchdb-adapter-node-websql": "^7.0.0",
    "pouchdb-find": "^8.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
