{
  "name": "mocody",
  "version": "0.0.83",
  "description": "Implementation of single table design, and unified query access for MongoDB, CouchDB, and dynamoDB",
  "esnext": "dist-src/index.js",
  "main": "dist-dist/index.js",
  "types": "dist-types/index.d.ts",
  "scripts": {
    "pub": "pika publish",
    "inst": "pika install",
    "build": "pika build",
    "version": "npm run build",
    "build-dev": "rimraf ./dist && tsc -b",
    "prettier-format-fix": "prettier --write \"src/**/*.{ts,js,json}\"",
    "prettier-different": "prettier --list-different \"src/**/*.{ts,js,json}\"",
    "prettier-unformatted": "prettier --check \"src/**/*.{ts,js,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genbliz/mocody.git"
  },
  "keywords": [
    "dynamodb",
    "mongodb",
    "couchdb",
    "orm",
    "single table design"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/genbliz/mocody/issues"
  },
  "homepage": "https://github.com/genbliz/mocody#readme",
  "files": [
    "dist-*/"
  ],
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "tsconfig": "tsconfig.json"
        }
      ],
      [
        "@pika/plugin-build-types"
      ],
      [
        "@pika/plugin-build-node"
      ]
    ]
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.4.1",
    "@aws-sdk/util-dynamodb": "^3.4.1",
    "joi": "^17.3.0",
    "lodash": "^4.17.21",
    "mongodb": "3.6.3",
    "nano": "^9.0.3",
    "throat": "^6.0.1",
    "tslib": "^2.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-types": "^0.9.2",
    "@pika/plugin-standard-pkg": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "^0.9.2",
    "@types/faker": "^5.1.7",
    "@types/lodash": "^4.14.168",
    "@types/mongodb": "^3.6.8",
    "@types/node": "^12.12.6",
    "@types/shortid": "0.0.29",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.3.1",
    "np": "^7.2.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
